{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/kafka-rabbitmq/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/kafka-rabbitmq/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/kafka-rabbitmq",
  "projects": {
    "dispatch": {
      "type": "application",
      "root": "apps/dispatch",
      "entryFile": "main",
      "sourceRoot": "apps/dispatch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dispatch/tsconfig.app.json"
      }
    },
    "events": {
      "type": "library",
      "root": "libs/events",
      "entryFile": "index",
      "sourceRoot": "libs/events/src",
      "compilerOptions": {
        "tsConfigPath": "libs/events/tsconfig.lib.json"
      }
    },
    "kafka": {
      "type": "library",
      "root": "libs/kafka",
      "entryFile": "index",
      "sourceRoot": "libs/kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka/tsconfig.lib.json"
      }
    },
    "kafka-rabbitmq": {
      "type": "application",
      "root": "apps/kafka-rabbitmq",
      "entryFile": "main",
      "sourceRoot": "apps/kafka-rabbitmq/src",
      "compilerOptions": {
        "tsConfigPath": "apps/kafka-rabbitmq/tsconfig.app.json"
      }
    },
    "rabbitmq": {
      "type": "library",
      "root": "libs/rabbitmq",
      "entryFile": "index",
      "sourceRoot": "libs/rabbitmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbitmq/tsconfig.lib.json"
      }
    },
    "recovery": {
      "type": "application",
      "root": "apps/recovery",
      "entryFile": "main",
      "sourceRoot": "apps/recovery/src",
      "compilerOptions": {
        "tsConfigPath": "apps/recovery/tsconfig.app.json"
      }
    },
    "worker": {
      "type": "application",
      "root": "apps/worker",
      "entryFile": "main",
      "sourceRoot": "apps/worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/worker/tsconfig.app.json"
      }
    }
  }
}